      <appender-ref ref="NHibernate.Profiler"/>
    </logger>
    <logger name="NHibernate.Event.Default.DefaultLoadEventListener">
      <level value="DEBUG"/>
      <appender-ref ref="NHibernate.Profiler"/>
    </logger>
    <logger name="NHibernate.Impl.AbstractSessionImpl">
