      <add name="Wrap Policy">
        <exceptionTypes>
          <!--<add type="NHibernate.HibernateException, NHibernate"
            postHandlingAction="ThrowNewException" name="HibernateException">
            <exceptionHandlers>
              <add exceptionMessage="出现数据库错误！" exceptionMessageResourceType=""
